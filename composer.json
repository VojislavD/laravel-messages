{
    "name": "vojislavd/laravel-messages",
    "description": "Laravel messages system",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "livewire/livewire": "^2.10",
        "orchestra/testbench": "^7.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "VojislavD\\LaravelMessages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VojislavD\\LaravelMessages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VojislavD\\LaravelMessages\\Providers\\LaravelMessagesServiceProvider",
                "VojislavD\\LaravelMessages\\Providers\\ActionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
